  <EnglishText/>
  <Notes/>
  <EditFlag>0</EditFlag>
 </Entry>
 <Entry>
  <MemoryOffsets>
   <MemoryOffset Module="Main">434</MemoryOffset>
  </MemoryOffsets>
  <LiteralPointers/>
  <MipsPointers/>
